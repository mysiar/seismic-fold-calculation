#!/usr/bin/python3
""" loads fold from csv file to db with checking every bin"""
import sys
import time
from sfc_app import usage, fold_db_update, timer

if __name__ == '__main__':
    if len(sys.argv) < 3:
        usage('project_file fold_csv_file')
        sys.exit(0)
    start = time.time()
    fold_db_update(sys.argv[1], sys.argv[2])
    sys.stdout.write(sys.argv[0] + '\n')
    sys.stdout.write(sys.argv[1] + '\n')
    sys.stdout.write(sys.argv[2] + '\n')
    sys.stdout.write(timer(start, time.time()) + '\n')
    sys.stdout.flush()
