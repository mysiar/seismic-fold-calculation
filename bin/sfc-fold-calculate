#!/usr/bin/python3
""" calculates fold and writes to csv """
import sys
import time
from sfc_app import usage, fold_calculate, timer

if __name__ == '__main__':
    if len(sys.argv) < 3:
        usage('project_file output_fold_csv_file')
        sys.exit(0)
    start = time.time()
    fold_calculate(sys.argv[1], sys.argv[2])
    sys.stdout.write(sys.argv[0] + '\n')
    sys.stdout.write(sys.argv[1] + '\n')
    sys.stdout.write(sys.argv[2] + '\n')
    sys.stdout.write(timer(start, time.time()) + '\n')
    sys.stdout.flush()
